<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <!-- Bootstrap CSS -->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/css/bootstrap.min.css"
      integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/init.css" />
    <link rel="stylesheet" href="/css/style.css" />
    <script src="http://pv.sohu.com/cityjson?ie=utf-8"></script>
    <script type="text/javascript"></script>
    <title>PICO CABINETS</title>
    <style>
      html {
        font-size: 16px;
      }
    </style>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg">
      <a class="navbar-brand" href="#">PICO CABINETS</a>
      <button
        class="navbar-toggler"
        type="button"
        data-toggle="collapse"
        data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto col-6">
          <li class="nav-item active">
            <a class="nav-link" href="#"
              >Home <span class="sr-only">(current)</span></a
            >
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Product</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Blog</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Contact</a>
          </li>
        </ul>
      </div>
    </nav>

    <!-- banner start -->
    <div class="row">
      <!-- <div class="col-md-2"></div> -->
      <div class="col-md-12">
        <div
          id="carouselExampleCaptions"
          class="carousel slide"
          data-ride="carousel"
        >
          <ol class="carousel-indicators">
            <li
              data-target="#carouselExampleCaptions"
              data-slide-to="0"
              class="active"
            ></li>
            <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
            <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>
          </ol>
          <div class="carousel-inner">
            <% if(banners.length) { banners.forEach(function(banner,i){ %>
              <% if(i==0){%>
                <div class="carousel-item active">
                  <%}else{  %>
                <div class="carousel-item">
               <%}%>
                  <img
                    src="<%= banner.image_uri %>"
                    class="d-block w-100"
                    alt="..."
                  />
                  <div class="carousel-caption d-none d-md-block">
                    <h5><%= banner.title %></h5>
                    <p><%- banner.content %></p>
                  </div>
                </div>
              <% })
            } %>
          </div>
          <a
            class="carousel-control-prev"
            href="#carouselExampleCaptions"
            role="button"
            data-slide="prev"
          >
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a
            class="carousel-control-next"
            href="#carouselExampleCaptions"
            role="button"
            data-slide="next"
          >
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
      <!-- <div class="col-md-2"></div> -->
    </div>

    <!-- banner end -->

    <!-- product start -->
    <h3>Product</h3>
    <div class="card-group product">
      <% if(products.length) { products.forEach(function(product){ %>

      <div class="card">
        <% if(product.image_uri){ %>
        <img src="  <%= product.image_uri %>" class="card-img-top" alt="..." />
        <% }else{ %>
        <img
          src="https://i.pinimg.com/564x/0f/f9/89/0ff9895712129bc3b5ca65f481c0f139.jpg"
          class="card-img-top"
          alt="..."
        />
        <% } %>
        <div class="card-body">
          <button type="button" class="btn btn-primary btn-lg btn-block">
            电子邮件
          </button>
        </div>
      </div>
      <% }) } %>
    </div>
    <!-- product end -->
    <!-- project start -->
    <h3>Project</h3>
    <div class="card-group certificate">
      <% if(projects.length){ projects.forEach((item)=>{ %>
      <div class="card">
        <img src="./img/banner1.jpg" class="card-img-top" alt="..." />
        <div class="card-body">
          <h5 class="card-title"><%=item.title%></h5>
          <p class="card-text"><%= item.intro %></p>
          <p class="card-text">
            <!-- <small class="text-muted">Last updated 3 mins ago</small> -->
          </p>
        </div>
      </div>
      <% }) } %>
    </div>
    <h3>Factory Ability</h3>
    <div class="card-group factory">
      <div class="card">
        <img src="<%= companyInfo.image_uri %>" class="card-img-top" alt="" />
      </div>
      <div class="card">
        <!-- <img src="./img/factory2.jpg" class="card-img-top" alt="" /> -->
        <div
          class="card-body data"
          style="background-image: url(<%= companyInfo.background_image_uri %>)"
        >
          <div class="cover"></div>
          <div class="number">
            <div>
              <div class="content"><%= companyInfo.data1 %></div>
              <div class="title">服务用户</div>
            </div>
            <div>
              <div class="content"><%= companyInfo.data2 %></div>
              <div class="title">服务用户</div>
            </div>
            <div>
              <div class="content"><%= companyInfo.data3 %></div>
              <div class="title">服务用户</div>
            </div>
            <div>
              <div class="content"><%= companyInfo.data4 %></div>
              <div class="title">服务用户</div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <h3>VIDEO TESTIMONIALS</h3>
    <div class="card-group product">
      <% if(cases.length){ cases.forEach((item)=>{ %>
      <div class="card">
        <img src="<%= item.image_uri%>" class="card-img-top" alt="..." />
        <div class="card-body"></div>
      </div>
      <%}) } %>
    </div>

    <div
      id="carouselExampleSlidesOnly"
      class="carousel slide footerPic"
      data-ride="carousel"
    >
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img
            src="<%= recommend.image_uri %>"
            class="d-block w-100"
            alt="..."
          />
          <div class="txt">
            <h3><%= recommend.title %></h3>
            <div class="intro"><%= recommend.content %></div>
            <div>
              <button type="button" class="btn btn-primary btn-lg">
                Large button
              </button>
              <button type="button" class="btn btn-primary btn-lg">
                Large button
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <footer class="footer">
      <div class="row">
        <div class="col-sm">
          <ul>
            <li>网站导航</li>
            <li>home</li>
            <li>product</li>
            <li>blog</li>
            <li>contactus</li>
          </ul>
        </div>
        <div class="col-sm">
          <ul>
            <li>CONTACT US</li>
            <li>phone: 15500000</li>
            <li>
              <a style="color: #fff" href="mailto:979797998@gmail.com">
                e-mail: 979797998@gmail.com
              </a>
            </li>
          </ul>
        </div>
      </div>
      <div class="statement">
        <div>
          使用條款|私隱權聲明|Copyright © 1995-2014 Samson Holding Ltd. All
          rights reserved
        </div>
        <div>資料由AASTOCKS提供 (免責聲明)</div>
      </div>
    </footer>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script
      src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js"
      integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
      integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/js/bootstrap.min.js"
      integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
